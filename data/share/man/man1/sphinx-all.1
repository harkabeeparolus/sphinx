app.add_js_file(\(aqexample.js\(aq, loading_method=\(dqasync\(dq)
# => <script src=\(dq_static/example.js\(dq async=\(dqasync\(dq></script>

app.add_js_file(None, body=\(dqvar myVariable = \(aqfoo\(aq;\(dq)
